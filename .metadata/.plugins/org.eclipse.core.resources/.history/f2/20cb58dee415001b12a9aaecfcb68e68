package myproject;

public class MyClass {

	public static void main(String[] args) {
	    int a=10,b=20,c=7;
	    if((a>b)&&(a>c))
	    {
	    	System.out.println("pass");	
	    }else if ((b>a)&&(b>c)) {
	    	System.out.println("correct");
	    }else if ((b>a)&&(c<b)) {
	    	System.out.println("fail");
	    }
	    else   {
	    	System.out.println(" invalid marks");
	    }
	    	
	}
	    	

	   
}

