package pageobjects;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class profileupload {
	public static WebElement MY PROFILE(WebDriver driver) {
		  return driver.findElement(By.xpath("\"//*[@class='nav-link go-text-right']\""));

	}
	public static WebElement Phone Num(WebDriver driver) {
		  return driver.findElement(By.name("phone"));

	}
	public static WebElement City(WebDriver driver) {
		  return driver.findElement(By.name("city"));

	}
	public static WebElement submit button(WebDriver driver) {
		  return driver.findElement(By.xpath("//*[@id=\\\"profilefrm\\\"]/div/div/div[7]/div/button"));

	}

}
