package myproject;

public class MyClass {

	public static void main(String[] args) {
	    int a=21,b=30,c=15;
	    if((a>b)&&(a>c))
	    {
	    	System.out.println("Highest number is:"+a);	
	    }else if ((b>a)&&(b>c)) {
	    	System.out.println("Highest number is:"+b);
	    }else   {
	    	System.out.println(" invalid marks");
	    }
	    	
	}
	    	

	   
}

